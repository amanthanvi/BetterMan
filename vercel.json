{
  "buildCommand": "cd frontend && npm install --force --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "env": {
    "FORCE_REBUILD": "2025-01-15-fix-store-and-urls"
  },
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*)\\.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/generated_manpages/**,backend/extracted_manpages/**"
    }
  }
}