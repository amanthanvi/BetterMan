{
  "version": 2,
  "buildCommand": "cd frontend && rm -rf node_modules package-lock.json .next .vercel/output && npm cache clean --force && npm install --force --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend/dist", 
  "installCommand": "cd frontend && npm install",
  "framework": null,
  "env": {
    "FORCE_REBUILD": "2025-01-15-removed-framer-motion"
  },
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*)\\.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health.py"
    },
    {
      "source": "/api/documents",
      "destination": "/api/documents.py"
    },
    {
      "source": "/api/documents/:id",
      "destination": "/api/documents.py"
    },
    {
      "source": "/api/search",
      "destination": "/api/search.py"
    },
    {
      "source": "/api/analytics/:path*",
      "destination": "/api/analytics.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@3.1.0",
      "includeFiles": "backend/generated_manpages/**,backend/extracted_manpages/**"
    }
  }
}